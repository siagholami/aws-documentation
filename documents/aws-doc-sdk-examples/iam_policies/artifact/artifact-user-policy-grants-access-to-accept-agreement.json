//snippet-sourcedescription:[This IAM identity-based permission policy demonstrates how to grant a user permission to accept an agreement.]
//snippet-keyword:[IAM Policy]
//snippet-keyword:[Code Sample]
//snippet-keyword:[AWS Artifact]
//snippet-service:[artifact]
//snippet-sourcetype:[full-example]
//snippet-sourcedate:[]
//snippet-sourceauthor:[AWS]
//snippet-start:[artifact.iam_policy.grants_attached_user_access_to_accept_an_agreement]
{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Action": [
                "artifact:AcceptAgreement"
            ],
            "Resource": [
                "*"
            ]
        }
    ]
}
//snippet-end:[artifact.iam_policy.grants_attached_user_access_to_accept_an_agreement]
