//snippet-sourcedescription:[This IAM identity-based permission policy demonstrates how to deny individual access to the billing console.]
//snippet-keyword:[IAM Policy]
//snippet-keyword:[Code Sample]
//snippet-keyword:[Billing and Cost Management]
//snippet-service:[billing]
//snippet-sourcetype:[full-example]
//snippet-sourcedate:[]
//snippet-sourceauthor:[AWS]
//snippet-start:[billing.iam_policy.denies_attached_user_access_to_billing_console]
{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Deny",
            "Action": "aws-portal:*",
            "Resource": "*"
        }
    ]
}
//snippet-end:[billing.iam_policy.denies_attached_user_access_to_billing_console]
